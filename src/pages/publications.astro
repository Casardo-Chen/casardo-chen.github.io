---
import BaseLayout from "../layouts/BaseLayout.astro";
import HorizontalCard from "../components/HorizontalCard.astro";
---

<BaseLayout title = "Publications" sideBarActiveItemID="publications">
  <div class="text-2xl w-full font-bold mb-2"  style="font-family: 'DM', sans-serif;" >Publications</div>
  <p class="mb-2">* denotes equal contribution</p>
  <div>
    <div class="text-2xl w-full font-bold">Conference Papers</div>
  </div>
  <HorizontalCard
    title="Developer Behaviors in Validating and Repairing LLM-Generated Code Using IDE and Eye Tracking"
    img="/publications/Eyetracking.png"
    authors={["Ningzhi Tang*", "Meng Chen*", "Zheng Ning", "Aakash Bansal", "Yu Huang", "Collin McMillan", "Toby Jia-Jun Li"]}
    venue="Proceedings of the 2024 IEEE Symposium on Visual Languages and Human-Centric Computing"
    venue_short="VL/HCC2024"
    tags={[["Paper","https://arxiv.org/abs/2405.16081"],]}
  />
  <!-- <div class="divider my-0 h-0"></div> -->
  <HorizontalCard
    title="Luminate: Structured Generation and Exploration of Design Space with Large Language Models for Human-AI Co-Creation"
    img="/publications/Luminate.jpg"
    authors={["Sangho Suh*", "Meng Chen*", "Bryan Min", "Toby Jia-Jun Li", "Haijun Xia"]}
    venue="Proceedings of the 2024 ACM Conference on Human Factors in Computing Systems"
    venue_short="CHI2024"
    tags={[["Website","https://luminate-research.github.io/"],["Video","https://www.youtube.com/watch?feature=shared&v=XMSGsq-COhI"],["Paper","https://arxiv.org/pdf/2310.12953.pdf"],]}
  />
  <!-- <div class="divider my-0 h-0"></div> -->
  <HorizontalCard
    title="A Bottom-Up End-User Intelligent Assistant Approach to Empower Gig Workers against AI Inequality"
    img="/publications/CHIWORK2022.png"
    authors= {["Toby Jia-Jun Li", "Yuwen Lu", "Jaylexia Clark", "Meng Chen", "Victor Cox", "Meng Jiang", "Yang Yang", "Tamara Kay", "Danielle Wood", "Jay Brockman"]}

    venue="Proceedings of the 1st ACM Symposium on Human-Computer Interaction for Work"
    venue_short="CHIWORK2022"
    tags={[["Video", "https://www.youtube.com/watch?v=8zCOj9G3I_o"], ["Paper","https://arxiv.org/pdf/2204.13842.pdf"]
    ]}
  />

  <div>
    <div class="text-2xl w-full font-bold mt-2">Extended Abstracts</div>
  </div>
  <HorizontalCard
    title="CodeGRITS: A Research Toolkit for Developer Behavior and Eye Tracking in IDE"
    authors={["Ningzhi Tang*", "Junwen An*", "Meng Chen", "Aakash Bansal", "Yu Huang", "Collin McMillan", "Toby Jia-Jun Li"]}
    url="https://kilthub.cmu.edu/articles/conference_contribution/An_Empirical_Study_of_Developer_Behaviors_for_Validating_and_Repairing_AI-Generated_Code/22223533/1/files/39505942.pdf"
    venue="Companion Proceedings of the 46th IEEE/ACM International Conference on Software Engineering"
    venue_short="ICSE2024 Demo"
    tags={[["Website","https://codegrits.github.io/CodeGRITS/"], ["Paper","https://codegrits.github.io/CodeGRITS/static/paper.pdf"],
    ]}
  />
  <!-- <div class="divider my-0 h-0"></div> -->
  <HorizontalCard
    title="An Empirical Study of Developer Behaviors for Validating and Repairing AI-Generated Code"
    authors={["Ningzhi Tang*", "Meng Chen*", "Zheng Ning", "Aakash Bansal", "Yu Huang", "Collin McMillan", "Toby Jia-Jun Li"]}
    url="https://kilthub.cmu.edu/articles/conference_contribution/An_Empirical_Study_of_Developer_Behaviors_for_Validating_and_Repairing_AI-Generated_Code/22223533/1/files/39505942.pdf"
    venue="13th Annual Workshop on the Intersection of HCI and PL
"
    venue_short="PLATEAU2023"
    tags={[["Paper","https://kilthub.cmu.edu/articles/conference_contribution/An_Empirical_Study_of_Developer_Behaviors_for_Validating_and_Repairing_AI-Generated_Code/22223533"],
    ]}
  />

</BaseLayout>
